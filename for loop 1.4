#include <stdio.h>

int main ()
{
    for (int i = 1; i <= 10; i++) {
        int limit;
        if (i <= 5) {
            limit = 6 - i;
        } else {
            limit = i - 5;
        }
        for (int j = 1; j <= limit; j++) {
            printf("%d ", j);
        }
        printf("\n");
    }
    return 0;
}

